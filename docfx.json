{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "bin/**/*.dll"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appLogoPath": "../media/BackOfficeDoorLogo.png",
      "_appFaviconPath": "..media/BackOfficeDoorLogo.png",
      "_appName": "BackOffice",
      "_disableNextArticle": false
    },
    "content": [
      {
        "files": [
          "parentLetterTemplateEditor/**.yml",
          "parentLetterTemplateEditor/index.md",
          "parentLetterTemplateEditor/coupon.md",
          "parentLetterTemplateEditor/footerImage.md",
          "parentLetterTemplateEditor/incentive.md",
          "parentLetterTemplateEditor/mailMerge.md",
          "parentLetterTemplateEditor/template.md",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "media/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": true
  }

}
